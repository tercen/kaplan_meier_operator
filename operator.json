{
  "name": "Kaplan-Meier",
  "description": "Fit a Kaplan-Meier curve to the input data.",
  "tags": ["statistical testing", "modelling"],
  "authors": ["tercen"],
  "urls": ["https://github.com/tercen/kaplan_meier_operator"],
  "container": "tercen/kaplan_meier_operator:0.0.3",
  "properties": [
    {
    "kind": "BooleanProperty",
    "name": "plot.ci",
    "defaultValue": true,
    "description": "Whether to plot confidence intervals."
    },
    {
    "kind": "BooleanProperty",
    "name": "plot.risk.table",
    "defaultValue": true,
    "description": "Whether to add a risk table to the plot."
    },
    {
    "kind": "DoubleProperty",
    "name": "width.scale.factor",
    "defaultValue": 1,
    "description": "Plot width scale factor."
    },
    {
    "kind": "DoubleProperty",
    "name": "heigth.scale.factor",
    "defaultValue": 1,
    "description": "Plot heigth scale factor."
    },
    {
    "kind": "DoubleProperty",
    "name": "text.scale.factor",
    "defaultValue": 1,
    "description": "Text size scale factor."
    }
  ]
}


